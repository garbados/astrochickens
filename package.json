{
  "name": "astrochickens",
  "version": "0.0.0",
  "description": "A space exploration game of massive scale.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jshint bin lib test index.js && mocha -R spec",
    "doc": "jsdoc lib -d doc -R readme.md",
    "build": "mkdir build; browserify lib/index.js -o build/app.js && cp -r lib/templates/*.html build/",
    "start": "npm run build && serve build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbados/astrochickens.git"
  },
  "keywords": [
    "game",
    "space"
  ],
  "author": "Diana Thayer <garbados@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garbados/astrochickens/issues"
  },
  "homepage": "https://github.com/garbados/astrochickens#readme",
  "devDependencies": {
    "browserify": "^10.2.4",
    "jsdoc": "^3.3.2",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "serve": "^1.4.0"
  },
  "dependencies": {
    "phaser": "^2.3.0"
  }
}
